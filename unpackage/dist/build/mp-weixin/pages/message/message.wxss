.doctor-card .doctor-card-pic{display:flex;align-items:center}.doctor-card .doctor-card-pic .card-message{display:flex;flex-wrap:nowrap;flex-direction:column;justify-content:space-evenly;margin-left:20rpx;margin-top:10rpx}.doctor-card .doctor-card-pic .card-message text:nth-child(2){margin:12rpx;color:#333;font-size:30rpx}.doctor-card .doctor-card-pic .card-message text:first-child{font-size:32rpx;font-family:PingFangSC-Medium,PingFang SC;font-weight:500;color:#000}.doctor-card .doctor-card-pic .card-message .uni-lastmsg{text-overflow:-o-ellipsis-lastline;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp:1;line-clamp:2;-webkit-box-orient:vertical;margin:4rpx}.doctor-card .doctor-card-pic .card-message .message-tips{margin-top:20rpx;display:flex;justify-content:space-between}.doctor-card .card-num{display:flex;justify-content:space-between}.doctor-card .card-num:nth-child(2){margin-top:25rpx}.doctor-card .card-num text{margin:4rpx}.doctor-card .card-num .mini-btn{border-radius:15px!important}.center-item{display:flex;align-items:center;justify-content:space-between}.center-item .card-item-add{display:flex}.center-item .card-item-add text{font-size:32rpx;color:#333;font-weight:500;margin-right:30rpx}.center-item .card-item-add view{font-size:28rpx}.center-item .fir-card-top{display:flex;flex-wrap:wrap;margin-top:1rpx}.center-item .fir-card-top text{margin-right:30rpx;font-size:28rpx}.card-user-info{margin:20rpx;padding:8rpx 60rpx;border-radius:20rpx;overflow:hidden;font-family:Helvetica Neue,Helvetica,PingFang SC,Hiragino Sans GB,Microsoft YaHei,SimSun,sans-serif;background-color:#fff;flex:1;border:2rpx solid #dcdcdc;line-height:62rpx}